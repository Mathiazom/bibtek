    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "java",
            "name": "CodeLens (Launch) - App",
            "request": "launch",
            "mainClass": "bibtek/bibtek.ui.App",
            "projectName": "bibtek",
            "vmArgs": "--module-path /path/to/JavaFX/lib --add-modules=javafx.controls"

        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-testJSON<bibtek>",
            "request": "launch",
            "mainClass": "bibtek.json.testJSON",
            "projectName": "bibtek",

        },
        {
            "type": "java",
            "name": "Debug (Launch)-App<bibtek>",
            "request": "launch",
            "mainClass": "bibtek.ui.App",
            "projectName": "bibtek",
            "vmArgs": "--module-path /path/to/JavaFX/lib --add-modules=javafx.controls"

        },
        {
            "type": "java",
            "name": "Debug (Launch)-ProviderUserApplication<microservice-simple-provider-user>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopOnEntry": false,
            "preLaunchTask": "package", //package before debug
            "mainClass": "com.itmuch.cloud.study.ProviderUserApplication",
            "projectName": "microservice-simple-provider-user",
            "args": "",
            "vmArgs": "--module-path /path/to/JavaFX/lib --add-modules=javafx.controls"

        }
    ]
}